{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "bundler",
    "sourceMap": true,
    "lib": ["esnext", "dom"],
    "baseUrl": ".",
    "types": ["vite/client", "unplugin-icons/types/vue"],
    "paths": {
      "@/*": ["src/*"]
    },
    "noImplicitAny": false,
    "noUncheckedIndexedAccess": false,
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "auto-imports.d.ts",
    "components.d.ts",
    "vite.config.ts"
  ]
}
