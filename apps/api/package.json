{
  "name": "api",
  "version": "0.1.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "codegen": "checkpoint generate",
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "yarn lint --fix",
    "build": "tsc",
    "dev": "nodemon src/index.ts",
    "dev:debug": "nodemon --exec \"node --inspect --require ts-node/register src/index.ts\"",
    "start": "node dist/src/index.js",
    "test": "vitest run"
  },
  "eslintConfig": {
    "extends": "@snapshot-labs",
    "rules": {
      "no-console": "error"
    }
  },
  "prettier": "@snapshot-labs/prettier-config",
  "dependencies": {
    "@apollo/server": "^4.2.2",
    "@faker-js/faker": "^7.4.0",
    "@logtail/pino": "^0.5.5",
    "@snapshot-labs/checkpoint": "^0.1.0-beta.67",
    "@snapshot-labs/sx": "^0.1.0",
    "@types/bn.js": "^5.1.0",
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.11.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.21.2",
    "graphql": "^16.8.1",
    "micro-starknet": "^0.2.3",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "starknet": "7.6.4",
    "viem": "^2.38.0"
  },
  "devDependencies": {
    "@snapshot-labs/eslint-config": "^0.1.0-beta.21",
    "@snapshot-labs/prettier-config": "^0.1.0-beta.19",
    "eslint": "^8.57.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
